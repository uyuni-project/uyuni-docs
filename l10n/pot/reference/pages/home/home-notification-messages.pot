msgid ""
"| Icon               | Description   | Example\n"
"| icon:info-circle[role=\"none\"] | Information | Client onboarding has "
"failed.\n"
"| icon:exclamation-triangle[role=\"orange\"] | Warning | Channel "
"synchronization has completed.\n"
"| icon:times-circle[role=\"red\"] | Error | Channel synchronization has "
"failed.\n"
msgstr ""
