msgid "The icons in this table indicate:"
msgstr ""

#. type: Plain text
#: modules/client-configuration/pages/supported-features.adoc:17
msgid "{check} clients running this operating system are supported by {suse}"
msgstr ""

#. type: Plain text
#: modules/client-configuration/pages/supported-features.adoc:18
msgid ""
"{cross} clients running this operating system are not supported by {suse}"
msgstr ""

#. type: Plain text
#: modules/client-configuration/pages/supported-features.adoc:19
msgid ""
"{question} clients are under consideration, and may or may not be supported "
"at a later date."
msgstr ""

#. type: delimited block =
#: modules/client-configuration/pages/supported-features.adoc:24
