#: modules/reference/pages/configuration/config-files-menu.adoc:9
#, no-wrap
msgid ""
"# /etc/rhn/rhn.conf\n"
"web.maximum_config_file_size = 262144\n"
"server.maximum_config_file_size = 262144\n"
msgstr ""

#. type: Plain text
#: modules/reference/pages/configuration/config-files-menu.adoc:10
msgid "Then restart [systemitem]``spacewalk``:"
