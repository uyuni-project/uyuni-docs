msgstr ""

#. type: Plain text
#: modules/reference/pages/spacecmd/activationkey.adoc:44
msgid "Enable an existing activation key."
msgstr ""

#. type: Plain text
#: modules/reference/pages/spacecmd/activationkey.adoc:45
msgid "usage: activationkey_enable KEY [KEY ...]"
msgstr ""

#. type: Title ==
#: modules/reference/pages/spacecmd/activationkey.adoc:46
#, no-wrap
msgid "activationkey_enableconfigdeployment"
msgstr ""

#. type: Plain text
#: modules/reference/pages/spacecmd/activationkey.adoc:47
msgid "Enable configuration channel deployment for an existing activation key."
msgstr ""

#. type: Plain text
#: modules/reference/pages/spacecmd/activationkey.adoc:48
msgid "usage: activationkey_enableconfigdeployment KEY"
msgstr ""

#. type: Title ==
#: modules/reference/pages/spacecmd/activationkey.adoc:49
#, no-wrap
msgid "activationkey_export"
msgstr ""

#. type: Plain text
#: modules/reference/pages/spacecmd/activationkey.adoc:50
msgid "Export activation keys to a JSON formatted file."
msgstr ""

#. type: Plain text
#: modules/reference/pages/spacecmd/activationkey.adoc:51
msgid "usage: activationkey_export [options] [<KEY> ...]"
msgstr ""

#. type: Plain text
#: modules/reference/pages/spacecmd/activationkey.adoc:52
