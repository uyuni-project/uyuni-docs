#: modules/reference/pages/spacecmd/activationkey.adoc:27
#, no-wrap
msgid ""
"options:\n"
"  -n NAME\n"
"  -d DESCRIPTION\n"
"  -b BASE_CHANNEL\n"
"  -u set key as universal default\n"
"  -e [enterprise_entitled,virtualization_host]\n"
msgstr ""

#. type: Title ==
#: modules/reference/pages/spacecmd/activationkey.adoc:28
#, no-wrap
msgid "activationkey_delete"
msgstr ""

#. type: Plain text
#: modules/reference/pages/spacecmd/activationkey.adoc:29
msgid "Delete an existing activation key."
msgstr ""

#. type: Plain text
#: modules/reference/pages/spacecmd/activationkey.adoc:30
msgid "usage: activationkey_delete KEY"
msgstr ""

#. type: Title ==
#: modules/reference/pages/spacecmd/activationkey.adoc:31
#, no-wrap
msgid "activationkey_details"
msgstr ""

#. type: Plain text
#: modules/reference/pages/spacecmd/activationkey.adoc:32
msgid "Show details of an existing activation key."
