#: modules/reference/pages/spacecmd/activationkey.adoc:37
#, no-wrap
msgid "activationkey_disable"
msgstr ""

#. type: Plain text
#: modules/reference/pages/spacecmd/activationkey.adoc:38
msgid "Disable an existing activation key."
msgstr ""

#. type: Plain text
#: modules/reference/pages/spacecmd/activationkey.adoc:39
msgid "usage: activationkey_disable KEY [KEY ...]"
