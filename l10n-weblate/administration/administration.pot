#. type: delimited block -
#: modules/administration/pages/backup-restore.adoc:139
#, no-wrap
msgid ""
"total 8\n"
"drwxr-x---  4 postgres postgres   47 Jun  2 14:35 ./\n"
msgstr ""

#. type: Plain text
#: modules/administration/pages/backup-restore.adoc:140
msgid ""
"Add the new database mount location to your servers fstab by editing "
"[path]``etc/fstab``."
msgstr ""

#. type: Plain text
#: modules/administration/pages/backup-restore.adoc:141
msgid "Start the database with:"
msgstr ""

#. type: delimited block -
#: modules/administration/pages/backup-restore.adoc:142
#: modules/administration/pages/backup-restore.adoc:158
#, no-wrap
msgid "rcpostgresql start\n"
msgstr ""

#. type: Plain text
#: modules/administration/pages/backup-restore.adoc:143
#: modules/administration/pages/backup-restore.adoc:155
msgid "Start the spacewalk services:"
msgstr ""

#. type: delimited block -
