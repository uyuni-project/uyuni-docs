#. type: delimited block -
#: modules/client-configuration/pages/clients-rh-rhui.adoc:78
#, no-wrap
msgid ""
"[<channel_label_1>]\n"
"X-RHUI-ID=<value>\n"
"X-RHUI-SIGNATURE=<value>\n"
msgstr ""

#. type: delimited block -
#: modules/client-configuration/pages/clients-rh-rhui.adoc:79
#, no-wrap
msgid ""
"[<channel_label_2>]\n"
"X-RHUI-ID=<value>\n"
"X-RHUI-SIGNATURE=<value>\n"
msgstr ""

#. type: Plain text
#: modules/client-configuration/pages/clients-rh-rhui.adoc:80
#, no-wrap
msgid "  Replace [literal]``<channel_label_X>`` above with channel names such as [literal]``rhel8-baseos-repo``:\n"
msgstr ""

#. type: delimited block -
#: modules/client-configuration/pages/clients-rh-rhui.adoc:81
#, no-wrap
msgid ""
"[rhel8-baseos-repo]\n"
"X-RHUI-ID=...\n"
"X-RHUI-SIGNATURE=...\n"
msgstr ""

#. type: delimited block =
#: modules/client-configuration/pages/clients-rh-rhui.adoc:89
msgid ""
