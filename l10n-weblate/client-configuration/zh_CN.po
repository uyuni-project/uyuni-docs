msgid ""
"server_id_use_crc: adler32\n"
"enable_legacy_startup_events: False\n"
"enable_fqdns_grains: False\n"
msgstr ""
"server_id_use_crc: adler32\n"
"enable_legacy_startup_events: False\n"
"enable_fqdns_grains: False\n"

#. type: Plain text
#: modules/client-configuration/pages/registration-cli.adoc:23
msgid "Restart the [systemitem]``venv-salt-minion`` service:"
msgstr "重启动 [systemitem]``venv-salt-minion`` 服务："

#. type: delimited block -
#: modules/client-configuration/pages/registration-cli.adoc:24
#, no-wrap
