msgstr "master: SERVER.EXAMPLE.COM\n"

#. type: delimited block -
#: modules/client-configuration/pages/registration-cli.adoc:21
#: modules/client-configuration/pages/registration-cli.adoc:40
#, no-wrap
msgid ""
"grains:\n"
"  susemanager:\n"
"    activation_key: \"<Activation_Key_Name>\"\n"
msgstr ""
"grains:\n"
"  susemanager:\n"
"    activation_key: \"<Activation_Key_Name>\"\n"

#. type: delimited block -
#: modules/client-configuration/pages/registration-cli.adoc:22
#: modules/client-configuration/pages/registration-cli.adoc:41
