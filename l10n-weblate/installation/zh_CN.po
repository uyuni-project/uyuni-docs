#: modules/installation/pages/install-proxy.adoc:23
#: modules/installation/pages/install-vm.adoc:27
#, no-wrap
msgid "rpm -e --nodeps sles-release\n"
msgstr "rpm -e --nodeps sles-release\n"

#. type: Plain text
#: modules/installation/pages/install-proxy.adoc:24
msgid ""
"Register {productname} Proxy with SCC (for example, replace "
"`<productversion>` with `{productnumber}` and `<architecture>` with "
"`x86_64`):"
msgstr ""
"将 {productname} Proxy 注册到 SCC（例如，将 `<productversion>` 替换为 "
"`{productnumber}`，并将 `<architecture>` 替换为 `x86_64`）："

#. type: delimited block -
#: modules/installation/pages/install-proxy.adoc:25
#, no-wrap
msgid ""
"SUSEConnect -e<EMAIL_ADDRESS> -r<SUSE_MANAGER_PROXY_CODE> \\\n"
"  -p SUSE-Manager-Proxy/<productversion>/<architecture>\n"
msgstr ""
"SUSEConnect -e<EMAIL_ADDRESS> -r<SUSE_MANAGER_PROXY_CODE> \\\n"
"  -p SUSE-Manager-Proxy/<productversion>/<architecture>\n"

#. type: Plain text
#: modules/installation/pages/install-proxy.adoc:26
#: modules/installation/pages/install-vm.adoc:30
msgid "Add {productname} repositories:"
msgstr "添加 {productname} 储存库"

#. type: delimited block -
#: modules/installation/pages/install-proxy.adoc:27
#, no-wrap
msgid ""
"SUSEConnect -p sle-module-basesystem/15.2/x86_64\n"
"SUSEConnect -p sle-module-server-applications/15.2/x86_64\n"
"SUSEConnect -p sle-module-suse-manager-proxy/4.1/x86_64\n"
msgstr ""
"SUSEConnect -p sle-module-basesystem/15.2/x86_64\n"
"SUSEConnect -p sle-module-server-applications/15.2/x86_64\n"
"SUSEConnect -p sle-module-suse-manager-proxy/4.1/x86_64\n"

#. type: Plain text
#: modules/installation/pages/install-proxy.adoc:28
msgid "Check that you have allowed installing recommended packages."
msgstr "检查是否允许安装建议的软件包。"

#. type: Plain text
#: modules/installation/pages/install-proxy.adoc:29
msgid "Check the settings in `/etc/zypp/zypp.conf`:"
msgstr "检查 `/etc/zypp/zypp.conf` 中的设置："

