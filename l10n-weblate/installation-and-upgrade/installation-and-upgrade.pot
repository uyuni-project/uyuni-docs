msgstr ""

#. type: Plain text
#: modules/installation-and-upgrade/pages/install-proxy-unified.adoc:8
msgid "Client systems see the {susemgrproxy} as the {productname} server instance."
msgstr ""

#. type: Plain text
#: modules/installation-and-upgrade/pages/install-proxy-unified.adoc:9
msgid ""
"The {susemgrproxy} is registered as a client system with the {productname} "
"server."
msgstr ""

#. type: Plain text
#: modules/installation-and-upgrade/pages/install-proxy-unified.adoc:10
msgid ""
"The primary goal of a {susemgrproxy} is to improve {productname} performance "
"by reducing bandwidth requirements and accelerating response time."
msgstr ""

