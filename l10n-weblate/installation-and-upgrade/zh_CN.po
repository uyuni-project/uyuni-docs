msgid "{scclongform} Account and Credentials"
msgstr "{scclongform} 帐户和身份凭证"

#. type: Plain text
#: modules/installation-and-upgrade/pages/general-requirements.adoc:9
msgid "Create an account with {scclongform} prior to deployment of {productname} {productnumber}."
msgstr "在部署 {productname} {productnumber} 之前，在 {scclongform} 中创建一个帐户。"

#. type: Block title
#: modules/installation-and-upgrade/pages/general-requirements.adoc:10
#, no-wrap
msgid "Procedure: Obtain Your Organization Credentials"
